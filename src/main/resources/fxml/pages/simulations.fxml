<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.fer.zemris.dipl.controllers.lists.SimulationListController">
    <children>
        <TableView fx:id="tableView" layoutX="14.0" layoutY="14.0" prefHeight="346.0" prefWidth="615.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="415.0" text="Simulation name" />
                <TableColumn fx:id="viewColumn" minWidth="0.0" prefWidth="66.0" text="View" />
                <TableColumn fx:id="runColumn" prefWidth="61.0" text="Run" />
                <TableColumn fx:id="removeColumn" prefWidth="71.0" text="Remove" />
            </columns>
        </TableView>
        <Button fx:id="createNewSimulation" layoutX="462.0" layoutY="366.0" mnemonicParsing="false" onAction="#switchToCreateNewSimulation" text="Create new simulation" />
      <Button onAction="#back" layoutX="14.0" layoutY="366.0" mnemonicParsing="false" text="Back" />
    </children>
</AnchorPane>
