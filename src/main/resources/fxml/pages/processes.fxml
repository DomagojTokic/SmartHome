<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.fer.zemris.dipl.controllers.lists.ProcessListController">
    <children>
        <Label layoutX="14.0" layoutY="12.0" text="Processes">
            <font>
                <Font name="System Bold" size="25.0" />
            </font>
        </Label>
        <TableView fx:id="tableView" layoutX="14.0" layoutY="50.0" prefHeight="344.0" prefWidth="624.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="194.0" text="Process name" />
                <TableColumn fx:id="descriptionColumn" minWidth="0.0" prefWidth="334.0" text="Description" />
                <TableColumn fx:id="removeColumn" minWidth="0.0" prefWidth="94.0" text="Remove" />
            </columns>
        </TableView>
        <Button fx:id="backBtn" layoutX="14.0" layoutY="402.0" mnemonicParsing="false" onAction="#back" text="Back" />
        <Button fx:id="createNewProcess" layoutX="471.0" layoutY="402.0" mnemonicParsing="false" onAction="#switchToCreateNewProcess" text="Create new process" />
      <Label layoutX="500.0" layoutY="30.0" text="(Double click to edit)" />

    </children>
</AnchorPane>
